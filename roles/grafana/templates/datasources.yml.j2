# {{ ansible_managed }}
apiVersion: 1

datasources:
  - name: Loki
    type: loki
    access: proxy
    url: http://loki:3100
    isDefault: true
    editable: false

  - name: Netdata
    type: prometheus
    access: proxy
    url: http://{{ target_control_ip }}:19999/api/v1/allmetrics?format=prometheus
    editable: false
